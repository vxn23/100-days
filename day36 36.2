Q72: Find the sum of all elements in a matrix.

/*
Sample Test Cases:
Input 1:
2 3
1 2 3
4 5 6
Output 1:
21

*/

//Solution:
#include <stdio.h>

int main() {
    int r, c, i, j, sum = 0;
    int mat[100][100];

    scanf("%d %d", &r, &c);
    for (i = 0; i < r; i++)
        for (j = 0; j < c; j++)
            scanf("%d", &mat[i][j]);

    for (i = 0; i < r; i++)
        for (j = 0; j < c; j++)
            sum += mat[i][j];

    printf("%d\n", sum);
    return 0;
}
